    "name": "Quentin Rousseau",
    "email": "contact@quent.in"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kwent/react-google-contacts/issues"
  },
  "homepage": "https://github.com/kwent/react-google-contacts",
  "dependencies": {
    "@types/react": "17.0.0",
    "buffer": "6.0.3",
