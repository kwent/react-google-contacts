    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint-config-ag": "3.1.0",
    "jest": "26.6.3",
    "pre-commit": "^1.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-fontawesome": "1.7.1",
    "react-hot-loader": "4.13.0",
    "react-test-renderer": "17.0.1",
    "webpack": "5.17.0",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2"
