    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "autoprefixer": "10.2.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
